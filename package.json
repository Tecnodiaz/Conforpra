{
  "name": "conforpra",
  "version": "1.0.0",
  "description": "\"Este es el proyecto de facturacion Conforpra\"",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder -c.extraMetadata.main=./index.js",
